<div><strong>news</strong></div>

<table class="table">
  <thead>
    
  </thead>
  <thead>
     　<tr>
        　<th>投稿者</th>
        　<th>投稿日時</th>
        　<th>タイトル</th>
          <th>いいね数</th>
          <th>コメント数</th>
    　　</tr>
  </thead>
  <tbody>
     　<tr>
         　<% @articles.each do |article| %>
         　 <td><%= link_to user_path(article.user) do %>
               <%= attachment_image_tag article.user, :profile_image, :fill, 100, 100, fallback: "no_image.jpg",  size: "100x100" %> <br>
         　     <%= article.user.name %>
         　     <% end %>
         　 </td>
         　<td> <%= article.created_at.strftime("%Y-%m-%d %H:%M") %></td>
         　<td><%= link_to article.title, article_path(article) %></td>
         　<td></td>
         　<td></td>
     　</tr>
     <% end %>
  </tbody>
</table>