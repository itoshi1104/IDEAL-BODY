<div class="container">
  <div class="col text-center">
    <div>記事投稿</div>
      <%= form_with model: @article, local:true do |f| %>
      <div>Title</div>
      <div><%= f.text_field :title %></div>
      <diV>本文</diV>
      <div><%= f.text_area :body %></div>
      <div><%= f.submit '投稿', class:"btn btn-success" %></div>
      <% end %>
  </div>
</div>